{
  "require": {
    "php": "7.3.*",
    "sensio/framework-extra-bundle": "5.*",
    "symfony/framework-bundle": "4.2.*",
    "symfony/flex": "^1.1",
    "symfony/dotenv": "4.2.*",
    "symfony/yaml": "4.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*.*",
    "php-coveralls/php-coveralls": "2.1.*",
    "symfony/browser-kit": "4.2.*",
    "symfony/console": "4.2.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "./tests"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
